%div{'data-href' => feed_path(feed), :class => "feed item_div", :id => "#{dom_id(feed)}_item"}
  = image_tag feed.avatar_url(:thumb), :class => "avatar"
  .title= feed.name

  - if current_user.feeds.include? feed
    %span.subscribed
      %span
        You're Subscribed
  - else
    - link_to feed_subscription_path(feed), 'data-remote' => true, 'data-method' => 'post', :class => "new_subscription new_#{dom_id feed}_subscription" do
      %span Subscribe?

