= javascript_include_tag "http://connect.facebook.net/en_US/all.js#xfbml=1"
%script
  FB.init({
  appId : "#{($FacebookConfig || {})['app_id']}",
  status : true,
  cookie : true,
  xfbml  : true
  });
  s = FB.getSession()
  = "FB.api('/#{@request_id}?access_token=#{@application_token}', 'GET', {access_token: '#{@application_token}'}, function(r) { console.log(r.data); });"
