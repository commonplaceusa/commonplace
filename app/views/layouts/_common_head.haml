- if community_name = current_community.try(:name) || params[:community]
  %meta{ :name => "description", :content => "The #{community_name} CommonPlace is an online community bulletin board for neighbors in #{community_name}, designed to make it easy for neighbors and community groups to share and connect." }
- else
