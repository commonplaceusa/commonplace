#fb-root
%script(type="text/javascript" src="http://connect.facebook.net/en_US/all.js")
:javascript
  $(document).ready(function() {
    FB.init({
      appId:'#{CONFIG['facebook_api_key']}', cookie: true,
      status: true, xfbml: true
    });
    FB.ui({
      method: 'feed', 
      message: 'Facebook for Websites is super-cool'
    });
  });