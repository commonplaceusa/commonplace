%div{'data-href' => feed_path(feed), :class => "feed tooltip", 'data-title' => 'Click to see more information about this feed'}
  = image_tag feed.avatar_url(:thumb), :class => "avatar"
  .title= feed.name

  - unless current_user.feeds.include? feed
    - link_to feed_subscription_path(feed), 'data-remote' => true, 'data-method' => 'post', :class => "new_subscription new_#{dom_id feed}_subscription" do
      %span Subscribe

