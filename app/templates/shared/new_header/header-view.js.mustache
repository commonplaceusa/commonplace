<div id="nav_wrapper">
  
  <div class="community-name">
    <a href="{{community_url}}">
      {{#isAuth}}
        {{#hasCommunity}}
          <h1 class="community">
            {{community_name}}
          </h1>
        {{/hasCommunity}}
      {{/isAuth}}
      {{#notAuth}}
        <img src="/assets/shared/redesign/logo3.png" alt="CommonPlace">
        {{#hasCommunity}}
          <span class="community">
            {{community_name}}
          </span>
        {{/hasCommunity}}
      {{/notAuth}}
    </a>
  </div>
  <div class="nav"></div>

</div>
