<div class="spacing">
  <div class="profile-main">
    <h3>{{fullName}}</h3>
    <img src="{{avatarUrl}}" width="122" />
    <div class="profile-top">
      <p>
      <span class="profile-number">23</span><br />
      <span class="profile-title">Posts</span><br />
      </p>
      <p>
      <span class="profile-number">18</span><br />
      <span class="profile-title">Replies</span><br />
      </p>
      <p>
      <div class="box-links">
      <a href="#" class="gray-button message">Send a Message</a>
      {{^hasMet}}<a href="#" class="gray-button meet">Have You Met?</a>{{/hasMet}}
      {{#hasMet}}<a href="#" class="gray-button unmeet">Don't Know Them?</a>{{/hasMet}}
  </div>
    </div>

  </div>
  <ul class="profile-data">
  {{#hasAbout}}<li>
    <h4>About</h4>
    <div class="about">{{#markdown}}{{about}}{{/markdown}}</div>
  </li>{{/hasAbout}}
  {{#hasInterests}}<li>
    <h4>Interests</h4>
    <ul> {{#interests}}<li class="interest">{{.}}</li>{{/interests}} </ul>
  </li>{{/hasInterests}}
  {{#hasSkills}}<li>
    <h4>Skills</h4>
    <ul> {{#skills}}<li class="interest">{{.}}</li>{{/skills}} </ul>
  </li>{{/hasSkills}}
  {{#hasGoods}}<li>
    <h4>Goods</h4>
    <ul> {{#goods}}<li class="interest">{{.}}</li>{{/goods}} </ul>
  </li>{{/hasGoods}}
  </ul>
</div>
